_pkgname=objgraph
pkgname=python-objgraph
pkgver=3.2.0
pkgrel=1
pkgdesc="Draws Python object reference graphs with graphviz"
arch=('any')
url="https://mg.pov.lt/objgraph/"
license=('MIT')
depends=('python' 'graphviz' 'python-setuptools')
source=('https://pypi.python.org/packages/89/dd/33ef81f7d6eb91c7805f1af169e9be145c4e65541e579d4938bb4bd74d9a/objgraph-3.2.0.tar.gz')
md5sums=('a4ced85049ea1b4f2f00d92b58d3535f')

build() {
  cd "$srcdir/$_pkgname-$pkgver"
  LANG=en_US.UTF-8 python3 setup.py build
}

package() {
  cd "$srcdir/$_pkgname-$pkgver"
  LANG=en_US.UTF-8 python3 setup.py install --root=$pkgdir --optimize=1 --skip-build
}

