pkgname='archlinuxcn-keyring'
pkgver='20141018'
pkgrel=2
pkgdesc='Arch Linux CN PGP keyring'
arch=('any')
url='https://github.com/archlinuxcn/repo'
license=('GPL')
install="archlinuxcn-keyring.install"
source=("https://github.com/hzy199411/archlinuxcn-keyring/archive/archlinuxcn-keyring-$pkgver.tar.gz")
