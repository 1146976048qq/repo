_pkgname=multidict
pkgname=python-multidict
pkgver=1.2.0
pkgrel=1
pkgdesc="multidict implementation"
arch=('i686' 'x86_64')
url="https://github.com/aio-libs/multidict/"
license=('Apache')
depends=('python' 'python-setuptools')
makedepends=('cython')
source=('https://pypi.python.org/packages/1b/8b/2d20d7e568b7f3adb686fad7717f04d198f7914a456ee07758872047706b/multidict-1.2.0.tar.gz')
md5sums=('999fdd9275a9b85581915160d59b4f44')

build() {
  cd "$srcdir/$_pkgname-$pkgver"
  python3 setup.py build
}

package() {
  cd "$srcdir/$_pkgname-$pkgver"

  python3 setup.py install --root=$pkgdir --optimize=1 --skip-build
}

